
public class Carro {
	private int potencia;
	private int velocidade;
	private int velocidadeMaxima;
	private String nome;
	
	void acelerar() {
		velocidade += potencia;
	}
	
	void frear() {
		velocidade = velocidade / 2;
	}
	
	int getVelocidade() {
		return velocidade;
	}
	
	void imprimir() {
		System.out.println("O carro "+nome+" está à velocidade de "+
				getVelocidade()+" km/h.");
	}
}
