import static org.junit.Assert.*;

import org.junit.Test;

public class Testes {

	@Test
	public void deposito() {
		ContaCorrente cc = new ContaCorrente();
		cc.depositar(200);
		assertEquals(200, cc.saldo);
	}
	
	@Test
	public void saque() {
		ContaCorrente cc = new ContaCorrente();
		cc.depositar(200);
		cc.sacar(50);
		assertEquals(150, cc.saldo);
		assertEquals(50, valorSacado);
	}

}
